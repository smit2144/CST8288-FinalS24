  </servlet>
  <servlet-mapping>
    <servlet-name>SurplusFoodIdentificationServlet</servlet-name>
    <url-pattern>/SurplusFoodIdentificationServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ListSurplusFoodItemsServlet</display-name>
    <servlet-name>ListSurplusFoodItemsServlet</servlet-name>
    <servlet-class>servlet.ListSurplusFoodItemsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ListSurplusFoodItemsServlet</servlet-name>
    <url-pattern>/ListSurplusFoodItemsServlet</url-pattern>
  </servlet-mapping>
</web-app>